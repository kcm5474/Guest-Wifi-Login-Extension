{
    "manifest_version": 3,
    "name": "Auto Click Accept and Connect",
    "version": "0.5",
    "description": "Automatically clicks the 'Accept and Connect' button on PSU Guest page.",
    "permissions": [],
    "host_permissions": [
        "https://psu-guest.psu.edu/*"
    ],
    "content_scripts": [
        {
            "matches": ["https://psu-guest.psu.edu/*"],
            "js": ["content.js"],
            "run_at": "document_idle"
        }
    ]
}
